<div class="container">
    <div class="row pt-9">
        <div class="col-6">
            <h2 class="text-primary">Employee List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-action="create" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> Add New Employee
            </a>
        </div>
    </div>
</div>

<table id="tblData" class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>Name</th>
            <th>Salary</th>
            <th>Designation</th>
            <th>CNIC</th>
            <th>Address</th>
            <th>Department</th>
            <th>Actions</th>
        </tr>
    </thead>
   
</table>
@section Scripts {
    <script src="~/js/Employee/Employees.js"></script>

}
